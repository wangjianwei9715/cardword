						if(uni.getSystemInfoSync().platform == 'ios'){
							this.k = i//Âà§Êñ≠‰∫ÜÁî®Êà∑Ê≤°ÊúâÊªëÂä®ÔºåÁ°ÆËÆ§‰∫ÜÁî®Êà∑ÁöÑÁ°ÆÊòØÂú®ÁúãËøô‰∏™ËßÜÈ¢ëÔºåÁÑ∂ÂêéÂ∞±ËµãÂÄºÂï¶
